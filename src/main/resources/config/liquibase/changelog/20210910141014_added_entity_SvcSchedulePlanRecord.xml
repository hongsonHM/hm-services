    <!--
        Added the entity SvcSchedulePlanRecord.
    -->
    <changeSet id="20210910141014-1" author="jhipster">
        <createTable tableName="svc_schedule_plan_record">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="svc_schedule_plan_id" type="integer">
                <constraints nullable="false" />
            </column>
            <column name="is_published" type="integer">
                <constraints nullable="false" />
            </column>
            <column name="status" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="approved" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="approved_id" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="comment" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="create_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="create_by" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="create_by_id" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_by" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="last_modified_by_id" type="integer">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

    <changeSet id="20210910141014-1-relations" author="jhipster">

        <createTable tableName="rel_svc_schedule_plan_record__svc_schedule_unit_record">
            <column name="svc_schedule_unit_record_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="svc_schedule_plan_record_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addPrimaryKey columnNames="svc_schedule_plan_record_id, svc_schedule_unit_record_id" tableName="rel_svc_schedule_plan_record__svc_schedule_unit_record"/>
    </changeSet>


<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity SvcContract.
    -->
    <changeSet id="20210731065405-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="approved_by_id"
                                 baseTableName="svc_contract"
                                 constraintName="fk_svc_contract__approved_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dvvs_user"/>

        <addForeignKeyConstraint baseColumnNames="owner_by_id"
                                 baseTableName="svc_contract"
                                 constraintName="fk_svc_contract__owner_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dvvs_user"/>

        <addForeignKeyConstraint baseColumnNames="unit_id"
                                 baseTableName="svc_contract"
                                 constraintName="fk_svc_contract__unit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="svc_unit"/>

        <addForeignKeyConstraint baseColumnNames="saler_id"
                                 baseTableName="svc_contract"
                                 constraintName="fk_svc_contract__saler_id"
                                 referencedColumnNames="id"
                                 referencedTableName="org_user"/>

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="svc_contract"
                                 constraintName="fk_svc_contract__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="svc_client"/>
    </changeSet>
</databaseChangeLog>
